{
  "kind": "collectionType",
  "collectionName": "blogs_categories",
  "info": {
    "singularName": "blogs-category",
    "pluralName": "blogs-categories",
    "displayName": "BlogCategory",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "shortDescription": {
      "type": "richtext"
    },
    "blogImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "autherName": {
      "type": "string"
    },
    "imageAlt": {
      "type": "string"
    },
    "autherImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "category"
    }
  }
}
